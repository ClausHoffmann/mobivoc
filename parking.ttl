@prefix mv: <http://purl.org/net/mobivoc/> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix skos: <http://www.w3.org/2004/02/skos/core#> .

# CLASSES

mv:ParkingFacility
	a rdfs:Class , owl:Class ; 
	rdfs:comment "A parking lot or other parking facility with several parking spaces for vehicles." ;
	rdfs:label "parking facility"@en , "Parkeinrichtung"@de .

mv:ParkingFacilityFeature
	a rdfs:Class , owl:Class ;
	rdfs:comment "Features of the parking facility (e.g. parking for handicapped people)." ;
	rdfs:label "parking facility feature"@en , "Ausstattung der Parkeinrichtung"@de .

mv:ParkingFacilityService
	a rdfs:Class , owl:Class ;
	rdfs:comment "Service available at the parking facility." #at
	rdfs:label "services of parking facility"@en , "Serviceangebot der Parkeinrichtung"@de .

#INSTANCES

## Parking Facility Features

mv:ParkingSpaceForDisabledPerson #man könnte ja auch die Anzahl der Behindertenparkplätze angeben wollen, dann brauch ich doch ein Konzept?
	a mv:ParkingFacilityFeature ;
	rdfs:label "parking space for disabled persons"@en , "Behindertenparkplatz"@de.

mv:LadiesParkingSpace
	a mv:ParkingFacilityFeature ;
	rdfs:label "ladies' parking space" , "Frauenparkplatz"@de .

mv:XXLParkingSpace
	a mv:ParkingFacilityFeature ;
	rdfs:label "XXL parking space" , "XXL-Parkplatz"@de ;
	skos:altLabel "extra-breiter Stellplatz"@de .

mv:VideoSurveillance
	a mv:ParkingFacilityFeature ;
	rdfs:label "video surveillance"@en , "Videoüberwachung"@de .

mv:SurveillanceBySecurityGuards #check englische Bezeichnung
	a mv:ParkingFacilityFeature ;
	rdfs:label "surveillance by security guards"@en , "Bewacht durch Personal"@de ;
	skos:altLabel "personenbewacht"@de . #prüfen, ob das so hieß

mv:PayStation
	a mv:ParkingFacilityFeature ;
	rdfs:label "pay station"@en , "Kassenautomat"@de .

mv:ParkingBarrier
	a mv:ParkingFacilityFeature ;
	skos:preflabel "parking barrier"@en , "Parkschranke"@de.
	skos:altlabel "park barrier"@en

## Parking Facility Services

mv:ChargingPoint #welche konsequenzen hat hier die verwendung von charging point?
	a mv:ParkingFacilityService
	# siehe ChargingPoint
	rdfs:label "charging point"@en , "Ladestation"@de .

mv:CarCleaningandMaintenance
	a mv:ParkingFacilityService ;
	rdfs:label "car cleaning and maintenance"@en , "Pkw-Reinigung und -Pflege"@de .


#PROPERTIES

mv:hasParkingFacilityFeatures
	a rdf:Property , owl:ObjectProperty ;
	rdfs:label "has parking facility feature" ;
	rdfs:comment "Indicates which features the parking facility provides."
	rdfs:domain mv:ParkingFacility ;
	rdfs:range mv:ParkingFacilityFeature .

mv:offersServices
	a rdf:Property , owl:ObjectProperty ;
	rdfs:label "offers services" ;
	rdfs:comment "Indicates which services are available at the parking facility." #at
	rdfs:domain mv:ParkingFacility ;
	rdfs:range mv:ParkingFacilityService .
