	a	rdf:Property, owl:ObjectProperty ;
	rdfs:label	"isOwnedBy"@en ;
	rdfs:comment	"." ;
	rdfs:domain	mv:ChargingPoint ;
	rdfs:range	mv:ChargingPointOwner .
	
